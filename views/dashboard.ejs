<%- include("header", {}) %>
<div class="container">
    <div class="container-fluid">
      <h2>Inicio</h2>
      <div class="container">
        <div class="row">
          <div class="col-lg">
            <h3>Calendario</h3>
            <div class="wrapper-calendar">
              <header>
                <p class="current-date"></p>
                <div class="icons">
                  <span id="prev" class="material-symbols-rounded">chevron_left</span>
                  <span id="next" class="material-symbols-rounded">chevron_right</span>
                </div>
              </header>
              <div class="calendar">
                <ul class="weeks">
                  <li>Sun</li>
                  <li>Mon</li>
                  <li>Tue</li>
                  <li>Wed</li>
                  <li>Thu</li>
                  <li>Fri</li>
                  <li>Sat</li>
                </ul>
                <ul class="days"></ul>
              </div>
            </div>
          </div>
          <div class="col-sm">
            <h3>Mis tareas</h3>
            <div class="wrapper-calendar">
              <div id="newtask">
                <input type="text" placeholder="Añade una tarea a tu día">
                <button class="btn btn-primary" id="push">Add</button>
              </div>
              <div class="container-task">
                <div id="tasks"></div>
              </div>
            </div>
          </div>
        </div><br>

        <div class="row">
          <h3>Eventos</h3>
          <div class="wrapper-calendar">
            <div id="carouselExampleCaptions" class="carousel slide">
              <div class="carousel-indicators">
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active"
                  aria-current="true" aria-label="Slide 1"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1"
                  aria-label="Slide 2"></button>
                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2"
                  aria-label="Slide 3"></button>
              </div>
              <div class="carousel-inner">
                <div class="carousel-item active">
                  <img src="images/ia.png" class="d-block w-100" alt="...">
                  <div class="carousel-caption d-none d-md-block">
                    <h4>Conferencia de Machine Learning</h4>
                  </div>
                </div>
                <div class="carousel-item">
                  <img src="images/aws2.jpg" class="d-block w-100" alt="...">
                  <div class="carousel-caption d-none d-md-block">
                    <h4>Charla Cloud Computing - AWS</h4>
                  </div>
                </div>
                <div class="carousel-item">
                  <img src="images/ciber.png" class="d-block w-100" alt="...">
                  <div class="carousel-caption d-none d-md-block">
                    <h4>Taller de Ciberseguridad</h4>
                  </div>
                </div>
              </div>
              <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions"
                data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
              </button>
              <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions"
                data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
              </button>
            </div>
          </div>
        </div>
      </div>
    </div><br>
  <script src="javascripts/script.js"></script>
  
<%- include("footer", {}) %>



